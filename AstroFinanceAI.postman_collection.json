    {
      "name": "Analysis",
      "item": [
        {
          "name": "Analyze Market (Basic)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"stocks\": [\n    {\n      \"symbol\": \"RELIANCE\",\n      \"sector\": \"Oil & Gas\",\n      \"past_6m_return\": 15.5,\n      \"volatility\": \"Medium\",\n      \"pe_ratio\": 24.2,\n      \"price_trend\": \"Upward\",\n      \"news_sentiment\": \"Positive\"\n    },\n    {\n      \"symbol\": \"TCS\",\n      \"sector\": \"Technology\",\n      \"past_6m_return\": 8.7,\n      \"volatility\": \"Low\",\n      \"pe_ratio\": 28.3,\n      \"price_trend\": \"Upward\",\n      \"news_sentiment\": \"Positive\"\n    }\n  ]\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{base_url}}/analyze",
              "host": ["{{base_url}}"],
              "path": ["analyze"]
            },
            "description": "Combines stock market data with planetary transits to generate AI-driven predictions"
          },
          "response": []
        },
        {
          "name": "Analyze Market (Enhanced)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"stocks\": null\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{base_url}}/analyze/enhanced?use_real_data=true",
              "host": ["{{base_url}}"],
              "path": ["analyze", "enhanced"],
              "query": [
                {
                  "key": "use_real_data",
                  "value": "true",
                  "description": "Use real market data from Alpha Vantage"
                }
              ]
            },
            "description": "Enhanced analysis with top recommendations and buy/hold/sell signals"
          },
          "response": []
        },
        {
          "name": "Analysis Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{base_url}}/analyze/health",
              "host": ["{{base_url}}"],
              "path": ["analyze", "health"]
